#!/bin/bash

cassandra_url="$OPENSHIFT_CASSANDRA_DB_USERNAME:$OPENSHIFT_CASSANDRA_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_CASSANDRA_DB_PROXY_PORT/"
connection_port_cassandra="$OPENSHIFT_CASSANDRA_NATIVE_PROXY_TRANSPORT_PORT"

echo "OPENSHIFT_CASSANDRA_DB_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_CASSANDRA_DB_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_CASSANDRA_DB_USERNAME=$OPENSHIFT_CASSANDRA_DB_USERNAME"
echo "OPENSHIFT_CASSANDRA_DB_PASSWORD=$OPENSHIFT_CASSANDRA_DB_PASSWORD"
echo "OPENSHIFT_CASSANDRA_DB_HOST=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_CASSANDRA_DB_PORT=$OPENSHIFT_CASSANDRA_DB_PROXY_PORT"
echo "OPENSHIFT_CASSANDRA_DB_URL=$cassandra_url"
echo "OPENSHIFT_CASSANDRA_NATIVE_PROXY_TRANSPORT_PORT=$OPENSHIFT_CASSANDRA_NATIVE_PROXY_TRANSPORT_PORT"

